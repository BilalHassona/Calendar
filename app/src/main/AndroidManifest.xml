<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calendarandroid">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher2"
        android:label=""
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:fullBackupContent="false"
        android:theme="@style/AppTheme2">
        <activity android:name=".MainActivity" android:label="@string/label_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FirstLaunchActivity"
            android:label="@string/title_activity_first_launch"
            android:theme="@style/AppTheme2.NoActionBar"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.calendarandroid.MainActivity">
            </meta-data>
        </activity>
        <activity
            android:name=".BasicActivity"
            android:label="@string/title_activity_basic" >
        </activity>
        <activity
            android:name=".AddEventActivity"
            android:label="@string/title_activity_add_event">
        </activity>
        <activity
            android:name=".CalendarActivity"
            android:label="">
        </activity>

    </application>

</manifest>

